<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Course Detail for @Model.GetName()</title>
  </head>
  <body>
    <h2>@Model.GetName() Details</h2>
    <h3>Students</h3>
    <ul>
      @foreach(var student in @Model.GetStudents())
      {
        <li>@student.GetName()</li>
      }
  </body>
</html>
